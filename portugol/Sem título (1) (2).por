programa {
  funcao inicio() {
    
    real presso
    real desconto
    leia(presso)
    se(presso >= 100){
      desconto=presso-(presso * 10)/100
      escreva("presso é maior ou igual 100 aplique um desconto 10%",desconto)
    }
    
    senao se(presso >= 50 )
      escreva("presso é maior ou igual 50 aplique um desconto 10%")
  
     }
  
  
  
  
  
  



      
}
