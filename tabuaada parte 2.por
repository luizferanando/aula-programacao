programa {
  funcao inicio() {
    inteiro tabuada =0
    escreva("tabuaada de 1 até o número digitado\n")
    escreva("digite o um número\n")
    leia(tabuada)

    para(inteiro i=1;i<=tabuada;i++){
     para(inteiro y=1;y<=10;y++){
      escreva(y,"X",i,":",(y*i),"\n")
      
    } escreva("\n")
     
  
  }
}
}